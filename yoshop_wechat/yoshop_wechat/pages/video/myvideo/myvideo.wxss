page{
  background:white;
}
.user{
  width:90%;
  margin:auto;
  display:flex;
  flex-direction: row;
  align-items: center;
  color:#000000;
  font-size:18pt;
  margin-top:10pt;
}
.user image{
  width:40pt;
  height:40pt;
  border-radius: 50%;
  margin-right:1pt;
}
.fb{
  font-size:15pt;
  color:#0058FF;
  margin-left:20pt;
}
.title{
  width:90%;
  margin:auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top:20pt;
  color:#999999;
}
.tt{
  height:20pt;
  text-align: center;
  line-height: 20pt;
}
.tts{
  height:20pt;
  text-align: center;
  line-height: 20pt;
  color:#0058FF;
  border-bottom: 1pt solid #0058FF;
}
.works{
  width:100%;
  margin-top:10pt;
  display:flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
}
.element{
  width:49.5%;
  height:200pt;
  margin-top:2pt;
}
.video{
  width:100%;
  height:200pt;
}